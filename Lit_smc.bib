@MastersThesis{Sachs2012,
  author    = {Julia Sachs},
  title     = {Model-Based Position Control of a Mechatronic Process Valve},
  year      = {2012},
  type      = {Diplomathesis},
  abstract  = {This thesis presents a model-based controller design
                  for the position control of a process valve. To
                  control the inherently nonlinear pneumatic system, a
                  combination of a flatnessbased feed forward
                  controller including a friction observer and a
                  feedback controller, is constructed. The
                  flatness-based feed forward controller is designed
                  using a parametrized model of the system, which can
                  be adjusted to other variants of valves by changing
                  the physical parameters. Thus, the controller is
                  applicable to the wide range of process valves
                  produced by Festo. With regard to the parameter
                  identification, a genetic algorithm and particle
                  swarm optimization are covered. Additionally, a
                  friction observer is constructed to cope with the
                  frictional effect. The inclusion of a pressure
                  observer in the friction observer enables the
                  estimation of the friction simply using the
                  actuating variable and the measured position. The
                  performance of the designed controller is
                  illustrated during numerical simulations in Matlab
                  and tested on a test bed. Besides a precise
                  point-to-point movement, an improved trajectory
                  tracking is achieved.},
  file      = {Sachs2012.pdf:_bibPACtrlEng\\Sachs2012.pdf:PDF},
  owner     = {fbwk},
  school    = {University of Stuttgart},
  timestamp = {2013.04.21},
}

